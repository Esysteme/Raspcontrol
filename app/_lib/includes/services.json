{ 
	"start Apache2": "sudo service apache2 start", 
	"stop Apache2": "sudo service apache2 stop", 
	"restart Apache2": "sudo service apache2 restart",
	"restart Network": "sudo /etc/init.d/networking restart",
	"restart SSH service": "sudo service ssh restart",
	"show last messages": "sudo tail --lines=50 /var/log/messages",
	"show systemlog": "sudo tail --lines=50 /var/log/syslog",
	"ls -ln": "ls -ln" 
}